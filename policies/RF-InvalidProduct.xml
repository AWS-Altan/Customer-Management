<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault name="RF-InvalidProduct" enabled="true" continueOnError="false" async="false">
    <DisplayName>RF-InvalidProduct</DisplayName>
    <Properties/>
    <FaultResponse>
        <Add>
            <Headers>
                <Header name="FaultHeader">InvalidProductError</Header>
            </Headers>
        </Add>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>