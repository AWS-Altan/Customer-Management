<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-ResponseCoordinates">
    <DisplayName>AM-ResponseCoordinates</DisplayName>
    <Set>
        <Payload variableSuffix="#" variablePrefix="@" contentType="application/json">
            {
                "latitude": "@latitude#",
                "longitude": "@longitude#"
            }
        </Payload>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo type="response" transport="https" createNew="true"/>
</AssignMessage>