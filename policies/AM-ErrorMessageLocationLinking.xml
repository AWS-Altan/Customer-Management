<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-ErrorMessageLocationLinking">
    <DisplayName>AM-ErrorMessageLocationLinking</DisplayName>
    <AssignVariable>
        <Name>errorMessage.status</Name>
        <Ref>err.errorCode</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>errorMessage.code</Name>
        <Ref>err.errorCode</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>errorMessage.message</Name>
        <Ref>err.description</Ref>
    </AssignVariable>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</AssignMessage>